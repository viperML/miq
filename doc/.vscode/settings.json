{
    "vim.disableExtension": true,
    "latex-workshop.latex.outDir": "%DIR%/out",
    "latex-workshop.latex.tools": [
        {
            "name": "just-build",
            "command": "just",
            "args": [
                "build"
            ],
        },
        {
            "name": "just-watch",
            "command": "just",
            "args": [
                "watch"
            ],
        },
        {
            "name": "just-sync",
            "command": "just",
            "args": [
                "sync"
            ],
        },
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": "build",
            "tools": [
                "just-sync",
                "just-build"
            ]
        },
        {
            "name": "watch",
            "tools": [
                "just-watch"
            ]
        },
    ],
    "cSpell.words": [
        "bootloader",
        "fetchable",
        "Guix",
        "libc",
        "libcrypto",
        "miku",
        "MINIX",
        "nginx",
        "openssl",
        "POSIX",
        "Ritchie",
        "serde",
        "stdenv",
        "structs",
        "userland"
    ]
}
